   "source": [
    "%matplotlib inline\n",
    "from mpl_toolkits.mplot3d import Axes3D\n",
    "\n",
    "theta_0_posterior = trace['theta_0']\n",
    "theta_1_posterior = trace['theta_1']\n",
    "\n",
    "fig = plt.figure(figsize=(11,5))\n",
    "\n",
    "x_b = np.linspace(0,20,n, endpoint = False)\n",
    "lambdaa1 =  theta_true[0]+theta_true[1]*x_b  # this is the model build up. \n",
    "r_b = np.random.poisson(lambdaa1)  # r_b is the counts generated from  poisson(lambda1)\n",
    "plt.plot(x_b,r_b, \"g.\")\n",
    "for i in range(B):\n",
    "    lambdaa11 = theta_0_posterior[i]+ theta_1_posterior[i]*x_b # Watch out! The parameters have changed. \n",
    "    plt.plot(x_b, lambdaa11, 'b', alpha=0.005)\n",
    "\n",
    "plt.ylim((0,50))\n",
    "plt.xlim((0,20))\n",
    "plt.xlabel(\"x\")\n",
    "plt.ylabel(\"r,counts\")\n",
    "\n",
    "plt.title(r\" uniform prior, poisson likelihood,  model : $ \\lambda = \\theta_{0}+ \\theta_{1} x $\")"
   ]
  },
