    "%matplotlib inline\n",
    "import pymc3 as pm\n",
    "from  IPython.core.pylabtools import figsize\n",
    "from pymc3 import Model,Normal,HalfNormal,find_MAP,Poisson,Metropolis,traceplot,sample,NUTS\n",
    "figsize(11,5)\n",
    "np.random.seed(1)\n",
    "with pm.Model() as model1:\n",
    "    theta_0 = pm.Uniform(\"theta_0\",lower=0,upper=10)\n",
    "    theta_1 = pm.Uniform(\"theta_1\",lower=0,upper=10)\n",
    "    lambdaa1 = theta_0 + theta_1*x_b\n",
    "    Y_obs   = pm.Poisson(\"Y_obs\",mu=lambdaa1,observed=r_b) #we treat observed data(x,r) as fixed value, then combine \n",
    "                       #p(λ|H) with likelihood function p(r|λ,H),by caculating the product of p(λ|H)*p(r|λ,H) to find MAP.\n",
    "    start = pm.find_MAP(fmin=optimize.fmin_powell)\n",
    "    step  = pm.NUTS(scaling=start)\n",
    "    trace = pm.sample(B ,step,start=start,progressbar=True)\n",
    "    pm.traceplot(trace)\n",
    "    pm.summary(trace)"
   ]
