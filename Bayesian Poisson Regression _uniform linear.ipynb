    "%matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "fig, axes = plt.subplots()\n",
    "plt.setp(axes,xticks = [0.,6.], xticklabels = [\"a\",\"b\"], yticks = [1,3], yticklabels=[\"1/(b-a)\",\"\"])\n",
    "plt.axhline(y=1,color = \"b\" ,linewidth = 3, label =\"$p(\\lambda$|H)\" )\n",
    "plt.legend(loc=\"best\")\n",
    "plt.xlabel(\"$\\lambda$\")\n",
    "plt.title(\"Prior distribution of $\\lambda$\")"
   ]
