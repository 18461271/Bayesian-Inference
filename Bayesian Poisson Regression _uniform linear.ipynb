    "%matplotlib inline\n",
    "import scipy.stats as stats\n",
    "import numpy as np\n",
    "plt.figure(figsize=(11,5))\n",
    "plt.setp(axes,xticks = [15], xticklabels = [\"$\\lambda$\"])\n",
    "x = range(60)\n",
    "y = stats.poisson.pmf(x,19)\n",
    "y2 =stats.poisson.pmf(x,38)\n",
    "\n",
    "plt.plot(y,\"b.\",label= \"$r \\sim $Poisson($\\lambda=19)$\")\n",
    "plt.plot(y2,\"r.\",label= \"$r \\sim $Poisson($\\lambda=38)$\")\n",
    "plt.vlines(19, 0, 0.09, color = \"black\", linestyles=\"--\", lw=1)\n",
    "plt.vlines(38,0,0.065, color = \"black\",linestyle=\"--\",lw=1)\n",
    "plt.xlabel(\"r\")\n",
    "plt.ylabel(r\"$p(r|\\lambda,H)$\")\n",
    "plt.title(\"Probability mass distribution of r, for fixed $\\lambda$\")\n",
    "plt.legend(loc=\"best\")\n",
    "plt.style.use(\"ggplot\")"
   ]
